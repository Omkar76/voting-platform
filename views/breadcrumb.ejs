<div class="flex-col flex justify-center gap-y-2.5">
  <nav class="inline-flex py-3 text-purple-800 rounded-lg" aria-label="Breadcrumb">
    <ol class="inline-flex items-center space-x-1 md:space-x-3">
      <li class="inline-flex items-center">
        <a href="/elections" class="inline-flex items-center text-sm font-medium text-purple-800 hover:text-blue-600">
          Elections
        </a>
      </li>

      <% 
         let route = "/elections";
         let endpoints = url.slice("/elections/".length).split("/").filter(e=>e)
         console.log(url, "bro")
      %>
      <%for(let endpoint of endpoints) { %>
        <% 
          route+= "/" + endpoint;
        %>
        <li>
          <div class="flex items-center">
            <svg aria-hidden="true" class="w-6 h-6 text-purple-800" fill="currentColor" viewBox="0 0 20 20"
              xmlns="http://www.w3.org/2000/svg">
              <path fill-rule="evenodd"
                d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z"
                clip-rule="evenodd"></path>
            </svg>
            <a href="<%= route %>" class="ml-1 text-sm font-medium text-purple-800 hover:text-blue-600 md:ml-2">
              <%= endpoint %>
            </a>
          </div>
        </li>
        <% } %>
    </ol>
  </nav>
</div>